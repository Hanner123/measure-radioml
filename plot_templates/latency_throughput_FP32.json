{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "title": "Latency (Sec) vs Throughput (sample/Sec)",
    "description": "Bar plot showing throughput vs batch size.",
    "data": {"values": "<DVC_METRIC_DATA>"},
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "latency_total",
        "type": "quantitative",
        "title": "Latency (Sec)"
      },
      "y": {
        "field": "throughput_images_per_s",
        "type": "quantitative",
        "title": "Throughput (sample/Sec)"
      }
    }
  }